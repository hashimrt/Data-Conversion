  <step>
    <name>Get-Input-LineItem's Invoices</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Input Connection</connection>
    <sql>SELECT
	CONVERT(NVARCHAR(MAX), CONVERT(BINARY(8), InvMovements.TimeStamp), 1) as currentTimeStamp,
	CONCAT(InvMovements.StockCode, InvMovements.Invoice) as unique_id,
	InvMovements.StockCode,
	InvMovements.Invoice,
	synced_invoices.sugarcrm_id as invoice_id,
	synced_lineitems.sugarcrm_id as lineitem_id

	FROM InvMovements
	LEFT JOIN EncoreCompany1_CRMBridge.dbo.synced_record as synced_invoices ON InvMovements.Invoice = synced_invoices.id AND synced_invoices.module = 'Invoices'
	LEFT JOIN EncoreCompany1_CRMBridge.dbo.synced_record as synced_lineitems ON InvMovements.StockCode = synced_lineitems.id AND synced_lineitems.module = 'LineItems'

	LEFT JOIN EncoreCompany1_CRMBridge.dbo.synced_record as sync_tbl ON sync_tbl.id = CONCAT(InvMovements.StockCode, InvMovements.Invoice) AND sync_tbl.module = 'LinkInvoices'

	WHERE (sync_tbl.id IS NULL OR CONVERT(NVARCHAR(MAX), CONVERT(BINARY(8), InvMovements.TimeStamp), 1) &lt;&gt; sync_tbl.synced_time_stamp) AND
	(sync_tbl.is_duplicate &lt;&gt; 1 OR sync_tbl.is_duplicate IS NULL) AND
	InvMovements.Invoice &lt;&gt; '' AND
	synced_invoices.sugarcrm_id &lt;&gt; '' AND
	synced_lineitems.sugarcrm_id &lt;&gt; ''
	ORDER BY InvMovements.Invoice ASC;</sql>
