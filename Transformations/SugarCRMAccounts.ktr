    <connection>Input Connection</connection>
    <sql>SELECT
	
 	CONVERT(NVARCHAR(MAX), CONVERT(BINARY(8), ArCustomer.TimeStamp), 1) as currentTimeStamp,
	ArCustomer.Customer,
	mainSyncRecord.id,
	mainSyncRecord.sugarcrm_id,
	mainSyncRecord.module,
	mainSyncRecord.is_synced,
	mainSyncRecord.is_duplicate
	FROM ArCustomer
	LEFT JOIN synced_record as mainSyncRecord ON mainSyncRecord.id = ArCustomer.Customer AND
	mainSyncRecord.module='Accounts'
	WHERE (mainSyncRecord.sugarcrm_id is NULL OR mainSyncRecord.sugarcrm_id='') AND mainSyncRecord.is_duplicate &lt;&gt;1
	ORDER BY ArCustomer.Customer ASC</sql>
    <limit>0</limit>
