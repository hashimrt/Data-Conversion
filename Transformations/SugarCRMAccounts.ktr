    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>var get_fields = {};
get_fields.view = 'list';
get_fields.max_num = '20';
get_fields['filter[0][acc_number_c]'] = Customer;

var out = [];

for (var key in get_fields) {
    if (get_fields.hasOwnProperty(key)) {
        out.push(key + '=' + encodeURIComponent(get_fields[key]));
    }
}
get_fields = out.join('&amp;');

var API_URL = CRM_BASE_URL+'/rest/v11_2/Accounts?'+ get_fields;</jsScript_script>
      </jsScript>
