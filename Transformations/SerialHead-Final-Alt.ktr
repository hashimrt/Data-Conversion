  <step>
    <name>Get-Input-SerialHead</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Input Connection</connection>
    <sql>SELECT 
		CONVERT(NVARCHAR(MAX), CONVERT(BINARY(8), InvSerialHead.TimeStamp), 1) as currentTimeStamp,
		CONCAT(InvSerialHead.StockCode, InvSerialHead.Serial) as unique_id,
		StockCode
      ,Warehouse
      ,Serial
      ,SerialDescription
      ,QtyReceived
      ,QtyAvailable
      ,QtyOnHand
      ,Location
      ,Customer
      ,TimeStamp
	  ,sync_tbl.sugarcrm_id
	  ,synced_lineitems.sugarcrm_id as lineitem_id

  FROM InvSerialHead
  
	LEFT JOIN EncoreCompany1_CRMBridge.dbo.synced_record as sync_tbl ON sync_tbl.id = CONCAT(InvSerialHead.StockCode, InvSerialHead.Serial) AND sync_tbl.module = 'SerialHead'
	LEFT JOIN EncoreCompany1_CRMBridge.dbo.synced_record as synced_lineitems ON InvSerialHead.StockCode = synced_lineitems.id AND synced_lineitems.module = 'LineItems'

 WHERE (sync_tbl.id IS NULL OR CONVERT(NVARCHAR(MAX), CONVERT(BINARY(8), InvSerialHead.TimeStamp), 1) &lt;&gt; sync_tbl.synced_time_stamp) AND
	(sync_tbl.is_duplicate &lt;&gt; 1 OR sync_tbl.is_duplicate IS NULL) AND
	synced_lineitems.sugarcrm_id &lt;&gt; ''
	
	ORDER BY InvSerialHead.Serial ASC</sql>
