    </partitioning>
    <connection>Input Connection</connection>
    <sql>SELECT
 	CONVERT(NVARCHAR(MAX), CONVERT(BINARY(8), ArCustomer.TimeStamp), 1) as currentTimeStamp,
	ArCustomer.*,
	mainSyncRecord.id,
	mainSyncRecord.sugarcrm_id,
	mainSyncRecord.module,
	mainSyncRecord.is_synced,
	mainSyncRecord.is_duplicate,
	mainSyncRecord.duplicate_sugarCRM_id,
	mainSyncRecord.synced_time_stamp,
	mainSyncRecord.sync_error,
	mainSyncRecord.error_field_name,
	mainSyncRecord.error_code,
	mainSyncRecord.no_of_errors
	FROM ArCustomer
	LEFT JOIN synced_record as mainSyncRecord ON mainSyncRecord.id = ArCustomer.Customer AND mainSyncRecord.module='Accounts'
	WHERE (mainSyncRecord.id IS NULL OR CONVERT(NVARCHAR(MAX), CONVERT(BINARY(8), ArCustomer.TimeStamp), 1) &lt;&gt; mainSyncRecord.synced_time_stamp) AND (mainSyncRecord.is_duplicate &lt;&gt; 1 OR mainSyncRecord.is_duplicate IS NULL)
	ORDER BY Customer ASC</sql>
